{
  "name": "beanstalk-api",
  "version": "0.0.1",
  "description": "",
  "main": "src/app.js",
  "scripts": {
    "start": "node ./src/app.js",
    "docker": "./docker/build.sh local && ./docker/start.sh local 3000 5432",
    "docker:stop": "./docker/stop.sh local",
    "docker:db": "./docker/start.sh local 3000 5432 postgres",
    "test": "jest",
    "debug": "node --inspect ./src/app.js",
    "prettier": "prettier --write .",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:create": "npx sequelize-cli migration:generate --name $1",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo",
    "seed:create": "npx sequelize-cli seed:generate --name $1"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFiles": [
      "<rootDir>/test/setup/jest.setup.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup/jest.setup-after.js"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@beanstalk/contract-storage": "^1.1.0",
    "@koa/cors": "^5.0.0",
    "alchemy-sdk": "^3.4.2",
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "ethers": "^6.12.1",
    "graphql-request": "^6.1.0",
    "koa": "^2.15.3",
    "koa-bodyparser": "^4.4.1",
    "koa-router": "^12.0.1",
    "node-cron": "^3.0.3",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.4",
    "sequelize-cli": "^6.6.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "prettier": "^3.3.2"
  },
  "private": true
}
